<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cs465.decisionhelper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DecisionHelperActivity"
            android:label="@string/decision_helper"></activity>
        <activity
            android:name=".PersonalityQuizActivity"
            android:label="@string/personality_quiz"></activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/settings"></activity>
        <activity
            android:name=".HelpActivity"
            android:label="@string/help"></activity>
    </application>

</manifest>